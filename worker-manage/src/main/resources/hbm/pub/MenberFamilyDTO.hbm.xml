<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pub.menber.model.dto.MenberFamilyDTO"
		table="pub_menber_family">
		<comment>会员亲友</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="menId" type="java.lang.String">
			<column name="MEN_ID" length="32">
				<comment>会员id</comment>
			</column>
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" length="100">
				<comment>被代管亲友姓名</comment>
			</column>
		</property>
		<property name="pid" type="java.lang.String">
			<column name="PID" length="100">
				<comment>被代管亲友身份证</comment>
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME">
				<comment>注册时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>