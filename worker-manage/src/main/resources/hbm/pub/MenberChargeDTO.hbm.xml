<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pub.menber.model.dto.MenberChargeDTO"
		table="pub_menber_charge">
		<comment>会员充值</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="menId" type="java.lang.String">
			<column name="MEN_ID" length="32">
				<comment>充值会员ID</comment>
			</column>
		</property>
		<property name="price" type="java.math.BigDecimal">
			<column name="PRICE" length="10">
				<comment>充值金额</comment>
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME">
				<comment>充值时间</comment>
			</column>
		</property>
		<property name="squeues" type="java.lang.String">
			<column name="SQUEUES" length="32">
				<comment>已支付返回标识</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>