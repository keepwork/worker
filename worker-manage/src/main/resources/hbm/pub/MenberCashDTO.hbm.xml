<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pub.menber.model.dto.MenberCashDTO"
		table="pub_menber_cash">
		<comment>会员提现</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="menId" type="java.lang.String">
			<column name="MEN_ID" length="32">
				<comment>提现会员ID</comment>
			</column>
		</property>
		<property name="price" type="java.math.BigDecimal">
			<column name="PRICE" length="10">
				<comment>提现金额</comment>
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME">
				<comment>提现时间</comment>
			</column>
		</property>
		<property name="isDown" type="java.lang.Integer">
			<column name="IS_DOWN" length="1">
				<comment>是否完成提现（1已完成，2未完成）</comment>
			</column>
		</property>
		<property name="auditTime" type="java.util.Date">
			<column name="AUDIT_TIME">
				<comment>审核时间</comment>
			</column>
		</property>
		<property name="auditor" type="java.lang.String">
			<column name="AUDITOR" length="32">
				<comment>审核人id</comment>
			</column>
		</property>
		<property name="comment" type="java.lang.String">
			<column name="COMMENT" length="32">
				<comment>审核说明</comment>
			</column>
		</property>
		
		<property name="bankUserName" type="java.lang.String">
			<column name="BANK_USER_NAME" length="100">
				<comment>收款人姓名</comment>
			</column>
		</property>
		<property name="bankName" type="java.lang.String">
			<column name="BANK_NAME" length="100">
				<comment>收款银行名称</comment>
			</column>
		</property>
		<property name="bankNum" type="java.lang.String">
			<column name="BANK_NUM" length="100">
				<comment>收款银行卡号</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>