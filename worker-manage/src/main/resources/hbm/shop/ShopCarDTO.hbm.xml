<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.shop.order.model.dto.ShopCarDTO"
		table="pub_shop_car">
		<comment>购物车</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="menId" type="java.lang.String">
			<column name="MEN_ID" length="32">
				<comment>会员id</comment>
			</column>
		</property>
		<property name="openId" type="java.lang.String">
			<column name="OPEN_ID" length="32">
				<comment>微信ID</comment>
			</column>
		</property>
		<property name="goodId" type="java.lang.String">
			<column name="GOOD_ID" length="32">
				<comment>商品编号</comment>
			</column>
		</property>
		<property name="goodName" type="java.lang.String">
			<column name="GOOD_NAME" length="300">
				<comment>商品名称</comment>
			</column>
		</property>
		<property name="goodPicture" type="java.lang.String">
			<column name="GOOD_PICTURE" length="300">
				<comment>商品图片</comment>
			</column>
		</property>
		<property name="price" type="java.math.BigDecimal">
			<column name="PRICE" length="10">
				<comment>总金额</comment>
			</column>
		</property>
		<property name="totalPrice" type="java.math.BigDecimal">
			<column name="TOTAL_PRICE" length="10">
				<comment>总金额</comment>
			</column>
		</property>
		<property name="priceShip" type="java.math.BigDecimal">
			<column name="PRICE_SHIP" length="10">
				<comment>运费</comment>
			</column>
		</property>
		<property name="point" type="java.lang.Integer">
			<column name="POINT" length="10">
				<comment>积分</comment>
			</column>
		</property>
		<property name="totalPoint" type="java.lang.Integer">
			<column name="TOTAL_POINT" length="10">
				<comment>总积分</comment>
			</column>
		</property>
		<property name="amount" type="java.lang.Integer">
			<column name="AMOUNT" length="10">
				<comment>数量</comment>
			</column>
		</property>
		<property name="orderType" type="java.lang.String">
			<column name="ORDER_TYPE" length="10">
				<comment>订单类型（1、微信订单；2、网站订单）</comment>
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME">
				<comment>加入时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>