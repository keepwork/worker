<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sinovatech.bms.adm.model.dto.TBmsLogOpDTO"
		table="sys_log_op">
		<comment>后台操作日志</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="userId" type="java.lang.String">
			<column name="USER_ID" length="32">
				<comment>操作人ID</comment>
			</column>
		</property>
		<property name="opMode" type="java.lang.String">
			<column name="OP_MODE" length="50">
				<comment>操作模块</comment>
			</column>
		</property>
		<property name="opType" type="java.lang.String">
			<column name="OP_TYPE" length="50">
				<comment>操作类型</comment>
			</column>
		</property>
		<property name="opTime" type="java.util.Date">
			<column name="OP_TIME">
				<comment>操作时间</comment>
			</column>
		</property>
		<property name="ip" type="java.lang.String">
			<column name="IP" length="15">
				<comment>操作人ip</comment>
			</column>
		</property>
		<property name="status" type="java.lang.String">
			<column name="STATUS" length="1">
				<comment>操作结果：1成功，0失败</comment>
			</column>
		</property>
		<property name="remark" type="java.lang.String">
			<column name="REMARK" length="400">
				<comment>备注</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>